<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="backgroundLogin" alignment="CENTER" maxHeight="680.0" maxWidth="400.0" minHeight="680.0" minWidth="400.0" prefHeight="680.0" prefWidth="400.0" spacing="5" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpackage.GUI.ControllerLogin">
   <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@LogoGelb.png" />
      <VBox.margin>
         <Insets bottom="30.0" />
      </VBox.margin>
   </ImageView>

            <Label prefWidth="220" text="EMAIL" />
            <TextField fx:id="tfEmailLogin" maxWidth="220" />

            <Label prefWidth="220" text="PASSWORD" />
            <PasswordField fx:id="tfPasswdLogin" maxWidth="220" />
   <Label fx:id="lbWarningLabel" alignment="CENTER" maxWidth="220.0" prefHeight="75.0" prefWidth="220.0" textAlignment="CENTER" />

            <Region prefHeight="10" />
            <Button minHeight="40" minWidth="120" onAction="#changeToDrinks" text="LOGIN" />
            <Region prefHeight="37.0" prefWidth="400.0" />
            <Label text="NOT SIGNED IN YET?" />
            <Button onAction="#changeToRegister" text="REGISTER" />
</VBox>
