<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="680.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpackage.GUI.ControllerDrinks">
   <children>
      <HBox alignment="CENTER" minHeight="120.0" prefHeight="120.0" prefWidth="200.0">
         <children>
            <ImageView fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@LogoLila.png" />
               </image>
            </ImageView>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <TabPane minHeight="500.0" minWidth="360.0" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                  <Tab text="COCKTAILS">
                      <content>
                          <ListView fx:id="listViewCocktails" prefHeight="100.0" prefWidth="200.0" />
                      </content>
                  </Tab>
                  <Tab text="SHOTS">
                      <content>
                          <ListView fx:id="listViewShots" prefHeight="100.0" prefWidth="200.0" />
                      </content>
                  </Tab>
                <Tab text="DRIVERDRINKS">
                    <content>
                        <ListView fx:id="listViewDriverDrinks" prefHeight="100.0" prefWidth="200.0" />
                    </content>
                </Tab>
              </tabs>
            </TabPane>
         </children></HBox>
      <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="menuButton1" mnemonicParsing="false" prefHeight="50.0" prefWidth="133.0" onAction="#logout">
                 <graphic>
                     <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                         <image>
                             <Image url="@insta.logo.schwarz.png" />
                         </image>
                     </ImageView>
                 </graphic>
             </Button>
            <Button fx:id="menuButton2" mnemonicParsing="false" prefHeight="50.0" prefWidth="133.0" text="Drinks" >
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@insta.logo.schwarz.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>
            <Button fx:id="menuButton3" mnemonicParsing="false" prefHeight="50.0" prefWidth="133.0" onAction="#changeToBasket" text="Basket" >
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                        <image>
                            <Image url="@insta.logo.schwarz.png" />
                        </image>
                    </ImageView>
                </graphic>
            </Button>
         </children></HBox>
   </children>
</VBox>
